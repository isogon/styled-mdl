{
  "name": "styled-mdl",
  "version": "0.1.1",
  "description": "Components for React built using Styled Components and based on Google's Material Design Lite",
  "private": false,
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/isogon/styled-mdl.git"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "author": "David Weedon",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn build:clean && yarn lint",
    "build": "babel -s inline --ignore '**/tests' --source-root src/ -d lib src/",
    "build:clean": "yarn test:clean && rimraf ./lib",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development node demo/server",
    "presetup": "yarn add global chalk shelljs",
    "setup": "yarn install",
    "postsetup": "yarn build:dll",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "yarn test:clean && yarn build:clean",
    "lint": "yarn lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "yarn lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "yarn test:clean && yarn lint",
    "test:clean": "rimraf ./coverage",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "manualPrepublish": "yarn build && prepare-isogon-package . && yarn generate-lock-entry --production > yarn.lock"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "dllPlugin": {
    "path": "node_modules/material-components-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "styled-components": "^2.2.2"
  },
  "dependencies": {
    "detect-node": "^2.0.3",
    "fontfaceobserver": "2.0.7",
    "lodash": "4.17.2",
    "prop-types": "^15.6.0",
    "react-modal": "^3.1.0",
    "react-portal": "^3.0.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@isogon/prepare-package": "^0.0.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "6.2.10",
    "babel-plugin-dynamic-import-node": "1.0.0",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "chalk": "^1.1.3",
    "cheerio": "0.22.0",
    "circular-dependency-plugin": "2.0.0",
    "compression": "^1.6.2",
    "core-decorators": "^0.15.0",
    "coveralls": "2.11.15",
    "cross-env": "^3.1.4",
    "css-loader": "^0.28.7",
    "enzyme": "^2.1.0",
    "eslint": "3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "express": "^4.14.1",
    "file-loader": "0.9.0",
    "history": "^3.0.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "ip": "^1.1.4",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^2.1.0",
    "lint-staged": "3.2.1",
    "minimist": "^1.2.0",
    "node-plop": "0.5.4",
    "null-loader": "0.1.1",
    "plop": "1.7.3",
    "pre-commit": "1.1.3",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "react-icons": "^2.2.7",
    "react-router": "^3.0.2",
    "react-router-scroll": "0.4.1",
    "react-syntax-highlighter": "^5.0.0",
    "rimraf": "2.5.4",
    "shelljs": "0.7.5",
    "sinon": "2.0.0-pre",
    "style-loader": "0.13.1",
    "styled-components": "^2.2.2",
    "url-loader": "0.5.7",
    "webpack": "^2.1.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0"
  }
}
